{
  "version": 2,
  "routes": [
    {
      "src": "/api/products/",
      "dest": "/server/api/products.js"
    }
  ],
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "nuxt.config.js",
      "use": "@nuxtjs/vercel-builder",
      "config": {
        "serverFiles": ["server/api/**"]
      }
    }
  ]
}
